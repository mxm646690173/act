<script>
import {linkTo} from "./index";

export default {
  name: "a-link",
  install() {
    this.render = props => {
      const VNode = props.children[0];
      VNode.attributes.onClick = _ => {
        return linkTo(props.to);
      }
      return props.children[0];
    }
  }
}
</script>
